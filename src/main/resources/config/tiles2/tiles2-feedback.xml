<tiles-definitions>

	<definition name="feedback.record.view" extends="default.layout">
		<put-attribute name="title" value="feedback.record.title" />
        <put-attribute name="body" value="/ftl/feedback/feedback.record.body.ftl" />
    </definition>
    
    <definition name="feedback.record.title" extends="default.title" >
       <put-attribute name="jsName" value="feedback/feedback.js"/>
    </definition>
    
    <definition name="feedback.record.table" template="/ftl/feedback/feedback.record.table.ftl" />

    
    <definition name="feedback.record.add" extends="default.layout">
		<put-attribute name="title" value="feedback.record.add.title" />
        <put-attribute name="body" value="/ftl/feedback/feedback.record.add.ftl" />
    </definition>
    
    <definition name="feedback.record.add.title" extends="default.title" >
       <put-attribute name="jsName" value="feedback/feedback.add.js"/>
